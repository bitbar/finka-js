{
  "source": {
    "include": [
      "src"
    ]
  },
  "opts": {
    "template": "node_modules/docdash",
    "encoding": "utf8",
    "destination": "docs",
    "recurse": true,
    "readme": "README.md",
    "package": ""
  },
  "docdash": {
    "search": true,
    "sectionOrder": [
      "Global",
      "Namespaces",
      "Classes"
    ],
    "collapse": true,
    "scripts": [
      "style.css"
    ],
    "menu": {
      "Github": {
        "href": "https://github.com/bitbar/finka-js",
        "target": "_blank",
        "class": "menu-item",
        "id": "website_link"
      },
      "NPM": {
        "href": "https://www.npmjs.com/package/@bitbar/finka",
        "target": "_blank",
        "class": "menu-item",
        "id": "forum_link"
      }
    }
  },
  "templates": {
    "name": "Finka.js",
    "footerText": "Copyright 2024 &copy; SmartBear Software",
    "default": {
      "staticFiles": {
        "include": [
          "./_static/style.css"
        ]
      }
    }
  }
}
